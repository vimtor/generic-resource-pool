{
  "name": "generic-resource-pool",
  "version": "0.0.0-semantic-release",
  "description": "Atomically access any object array",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./redis": {
      "require": "./dist/drivers/redis.js",
      "import": "./dist/drivers/redis.mjs"
    },
    "./dynamodb": {
      "require": "./dist/drivers/dynamodb.js",
      "import": "./dist/drivers/dynamodb.mjs"
    }
  },
  "author": "vimtor",
  "license": "MIT",
  "homepage": "https://github.com/vimtor/generic-resource-pool",
  "repository": {
    "url": "https://github.com/vimtor/generic-resource-pool.git"
  },
  "scripts": {
    "build": "tsup src/** --minify --format cjs,esm --dts --sourcemap",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "ioredis": "^5",
    "@aws-sdk/client-dynamodb": "^3"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    },
    "@aws-sdk/client-dynamodb": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.10.0"
}
